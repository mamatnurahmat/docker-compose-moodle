<div class="menu-picker-select">
    <button class="btn coursemenubtn p-0 d-flex align-items-center justify-content-center" type="button" aria-expanded="false" data-toolbar='#manage-course-{{id}}'>
        <span class="edw-icon edw-icon-more" aria-hidden="true" aria-label=""></span>
        <span class="edw-icon edw-icon-Cancel d-none" aria-hidden="true" aria-label=""></span>
    </button>
    <div id="manage-course-{{id}}" class="menu-content d-none">
       <a href="{{enrollink}}{{id}}&version=5" class="btn btn-primary manage-course-menu" title="{{enroltitle}}">
            <i class="edw-icon edw-icon-Group-user1" aria-hidden="true"></i>
        </a>
        <a href="{{graderreportlink}}{{id}}" class="btn btn-primary manage-course-menu" title="{{graderreporttitle}}">
            <i class="edw-icon edw-icon-Rating-stars_Active" aria-hidden="true"></i>
        </a>
        <a href="{{activityreportlink}}{{id}}" class="btn btn-primary manage-course-menu" title="{{activityreporttitle}}">
            <i class="edw-icon fa fa-pie-chart" aria-hidden="true"></i>
        </a>
        <a href="{{editcourselink}}{{id}}" class="btn btn-primary manage-course-menu" title="{{editcoursetitle}}">
            <i class="edw-icon edw-icon-Edit" aria-hidden="true"></i>
        </a>
        <a href="#" class="btn btn-primary manage-course-menu remui-view-course-report" data-course-id="{{id}}" title="{{coursereporttitle}}">
            <i class="edw-icon fa fa-table" aria-hidden="true"></i>
        </a>
    </div>
</div>
